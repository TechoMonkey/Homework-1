//
#include<iostream>
#include<string>
#include <regex>

using namespace std;

void ProblemOne(int a, int b)
{
	int count = a;
	while (count <= 1000)
	{
		while (count % b == 0)
		{
			cout << count << '\t';
			break;
		}
		count++;
	}
}
void ProblemTwo(string original)
{
    
 

    //Initialize reverse
    string rev = "";

    //Iterate input string in revrese order
    for (int i = original.length() - 1; i >= 0; i--) {
       
        rev = rev + original[i];
    }
	cout << original<< endl;
    cout << rev;
 
}
void ProblemThree(string str)
{
	string result = "";
	\\ 43 is making an array that seperates the letters then it goes through a if statement
	for (int i = 0; 1 < str.length() i++) {
       
        bool isVowel = false;
	
	if(str[i]=="a")
	{
	isVowel = true;
	}
    }
   return result;
}


int main()
{
	cout << "Problem One"<< endl;
	ProblemOne(5,10);
	cout << endl <<"Problem Two" << endl;
	ProblemTwo("Hello World");
	cout << endl<< "Problem Three" << endl;
	ProblemThree("the cake is a lie");
	
	
	return 0;
}

